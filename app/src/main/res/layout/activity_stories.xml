<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".stories.Stories"
    android:orientation="vertical"
    android:background="@drawable/backgroundstories">
    <ImageView
        android:id="@+id/img_anbya_stories"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:src="@drawable/anbyastories"
        android:layout_alignParentLeft="true"
        android:layout_toLeftOf="@id/img_edu_stories"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="50dp"/>
    <ImageView
        android:id="@+id/img_edu_stories"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:src="@drawable/edustoriesimg"
       android:layout_centerHorizontal="true"
        android:layout_marginVertical="100dp" />
    <ImageView
        android:id="@+id/img_ent_stories"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_toRightOf="@+id/img_edu_stories"
       android:layout_marginTop="50dp"

        android:src="@drawable/entstories" />


</RelativeLayout>
